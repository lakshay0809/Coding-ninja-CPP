#include<iostream>
using namespace std;

int main() {
	// Write your code here
    int n,num,count=1;
    cin >> n;
    int x=1;
    while(count <=n)
    {
        num = 3*x + 2;
        if(num % 4!=0)
        { 
            count ++;
            cout << num << ' ';
        }
        
        x++;
    }
	
}
