void sortZeroesAndOne(int *arr, int size)
{
    //Write your code here
   int i,j,c=0;
    for(int i=0;i<size;i++)
    {
        if(arr[i]==0)
        {
            c++;
        }
    }
    for(int i=0;i<c;i++)
    {
        arr[i] = 0;
    }
    for(int j=c;j<size;j++)
    {
        arr[j]=1;
    }
}
