#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
    int n,x=1;
    cin >> n;
    for(int i=1;i<=n;i++)
    {
         x=i;
        for(int j=1;j<=i;j++)
        {
           
            cout << x;
            x++;
        }
        cout<<endl;
        
    }
  
}
