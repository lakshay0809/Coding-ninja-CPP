#include<cmath>
#include<iostream>
using namespace std;

int main() {
	// Write your code here
    double n,hra,da,al,pf,ts;
    int tst;
    char c;
    cin>>n>>c;
    hra = 0.2*n;
    da=0.5*n;
    if(c=='A')
    {
        al=1700;
    }
    else if(c=='B') 
    {
        al=1500;
    }
    else{
        al=1300;
    }
    pf=0.11*n;
    
    ts=n+hra+da+al-pf;
    tst=round(ts);
    cout << tst;
	
}
