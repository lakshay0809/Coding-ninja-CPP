#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
     int s,e,w,c=0;
    cin >> s >> e >> w;
    for (int i=s;i<=e;i+=w)
    {
        c=5.0/9*(i-32);
        cout<<i <<"\t"<<c<<endl;
    }
    
}
