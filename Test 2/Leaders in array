void Leaders(int* a,int n)
{
	/* Don't write main().
	 * Don't read input, it is passed as function argument.
	 * Print your output exactly in the same format as shown. 
	 * Don't print any extra line.
	*/
    

    int j=0, max, maxi;
    while(j<n){
        max=0;
        for(; j<n; j++){
            if(a[j]>max){
                max = a[j];
                maxi = j;
            }
        }
        cout<<max<<" ";
        j=maxi+1;
    }
   
  
}
