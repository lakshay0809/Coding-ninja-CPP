#include <iostream>
using namespace std;

void print2DArray(int **arr, int n, int m) {

  int i=0,c=0;
    int temp = n;
    while(i<n)
    {
        for(int j=0;j<m;j++)
        {
            cout << arr[i][j] << ' ';
        }
        c++;
        cout << endl;
        if(c==temp)
        {
            c=0;
            temp--;
            i++;
        }
    }

}
