void wavePrint(int **input, int n, int m)
{
    //Write your code here
    for(int j=0;j<m;j++)
    {
        if(j%2!=0)
        {
            for(int i=n-1;i>=0;i--)
            {
                cout << input[i][j] << ' ';
            }
        }
        else 
        {
          for(int i=0;i<n;i++)
          {
              cout << input[i][j] << ' ';
          }
        }
    }
}
