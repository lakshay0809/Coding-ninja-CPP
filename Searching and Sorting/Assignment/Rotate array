void rotate(int *arr, int d, int n)
{
    //Write your code here
    int a[d];
    for(int i=0;i<d;i++)
    {
        a[i] = arr[i];
        
    }
    for(int i=0;i<n;i++)
    {
        arr[i] = arr[i+d];
    }
    int x=0;
    for(int  i=n-d;i<n;i++)
    {
        arr[i]= a[x];
        x++;
    }
   
}
